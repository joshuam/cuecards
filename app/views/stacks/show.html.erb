<p id="notice"><%= notice %></p>

<p>
  <b>Stack Name:</b>
  <%= @stack.name %><br />
  <b>Cards:</b>
  <ul>
  <% @stack.cards.each do |card| %>
    <li><%= card.item %></li>
  <% end %>
  </ul>
</p>
<%= link_to 'Add Card', new_card_path %>
<%= link_to 'Edit', edit_stack_path(@stack) %> |
<%= link_to 'Back', stacks_path %>
